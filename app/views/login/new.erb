<%= render 'shared/errors' %>
<div data-turbo="false">
<%= form_for :user, class: 'user_data_form' do |f| %>
  <div class="input_item">
    <%= label_tag :email, 'Email' %>
    <%= f.email_field :email %>
  </div>
  <div class="input_item">
    <%= label_tag :password, 'Пароль' %>
    <%= f.password_field :password %>
  </div>
  <div class="submit_button">
    <%= f.submit 'Войти' %>
  </div>
<% end %>
</div>